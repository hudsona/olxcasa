    <sql>select count&#x28;&#x2a;&#x29; as count_total&#xa;	  ,count&#x28;distinct ad_id&#x29; as count_distinct&#xa;      ,&#x27;ads_approval&#x27; as subject&#xa;	  ,&#x27;blocket&#x27; as schema&#xa;	  ,now&#x28;&#x29; as insert_date&#xa;      ,&#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; as date_from&#xa;	  ,&#x27;&#x24;&#x7b;DATE_TO&#x7d;&#x27; as date_to&#xa;from &#x28;&#xa;select aa.ad_id, a.list_id, a.user_id, a.account_id,u.email,acts.timestamp as approval_date,a.category, a.region,a.type,a.company_ad,a.price,aa.action_type&#xa;from &#x24;&#x7b;CURRENT_SCHEMA&#x7d;.action_states as acts, &#x24;&#x7b;CURRENT_SCHEMA&#x7d;.ad_actions as aa&#xa;left outer join &#x24;&#x7b;CURRENT_SCHEMA&#x7d;.ads as a on aa.ad_id &#x3d; a.ad_id&#xa;left outer join &#x24;&#x7b;CURRENT_SCHEMA&#x7d;.action_params as ap on ap.ad_id &#x3d; aa.ad_id and ap.action_id &#x3d; aa.action_id and ap.name &#x3d; &#x27;source&#x27;&#xa;left outer join public.users as u on u.user_id &#x3d; a.user_id&#xa;where aa.ad_id &#x3d; acts.ad_id and aa.action_id &#x3d; acts.action_id&#xa;and acts.state &#x3d; &#x27;accepted&#x27;&#xa;and acts.timestamp between &#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; and &#x27;&#x24;&#x7b;DATE_TO&#x7d;&#x27;&#xa;union&#xa;select aa.ad_id, a.list_id, a.user_id, a.account_id,u.email,acts.timestamp as approval_date,a.category, a.region,a.type,a.company_ad,a.price,aa.action_type&#xa;from public.action_states as acts, public.ad_actions as aa&#xa;left outer join public.ads as a on aa.ad_id &#x3d; a.ad_id&#xa;left outer join public.action_params as ap on ap.ad_id &#x3d; aa.ad_id and ap.action_id &#x3d; aa.action_id and ap.name &#x3d; &#x27;source&#x27;&#xa;left outer join public.users as u on u.user_id &#x3d; a.user_id&#xa;where aa.ad_id &#x3d; acts.ad_id and aa.action_id &#x3d; acts.action_id&#xa;and acts.state &#x3d; &#x27;accepted&#x27;&#xa;&#x29; as tab&#xa;where approval_date between &#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; and &#x27;&#x24;&#x7b;DATE_TO&#x7d;&#x27;</sql>
